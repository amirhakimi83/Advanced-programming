<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: 0;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.Timeline">
   <children>
      <ScrollPane fx:id="myScrollpane" layoutY="100.0" prefHeight="350.0" prefWidth="600.0">
        <content>
          <AnchorPane fx:id="myAnchorpane" minHeight="0.0" minWidth="0.0" prefHeight="800.0" prefWidth="500.0" />
        </content>
      </ScrollPane>
      <ImageView fitHeight="76.0" fitWidth="79.0" layoutX="261.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@download.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="64.0" fitWidth="60.0" layoutX="14.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true" style="-fx-view-order: 50;">
         <image>
            <Image url="@avatar.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" onMouseClicked="#goToProfile" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="newTweet" contentDisplay="CENTER" layoutX="512.0" layoutY="35.0" mnemonicParsing="false" onAction="#openNewTweet" text="Button" />
      <ImageView fx:id="imageView" fitHeight="77.0" fitWidth="94.0" layoutX="501.0" layoutY="9.0" onMouseClicked="#openNewTweet" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@download%20(2).png" />
         </image>
      </ImageView>
      <ImageView fitHeight="31.0" fitWidth="54.0" layoutX="550.0" layoutY="455.0" onMouseClicked="#goToSearch" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@search-icon.png" />
         </image>
         <effect>
            <DropShadow>
               <color>
                  <Color red="1.0" green="1.0" blue="1.0" />
               </color>
            </DropShadow>
         </effect>
      </ImageView>
   </children>
</AnchorPane>
